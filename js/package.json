    "randomatic": "3.1.1",
    "rollup": "3.23.0",
    "rxjs": "7.8.1",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typedoc": "0.24.7",
    "typescript": "5.0.4",
    "vinyl-buffer": "1.0.1",
    "vinyl-named": "1.1.0",
